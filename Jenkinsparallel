pipeline
{
agent any
stages
{
stage('scm checout')
{
steps {
git branch: 'master', url: 'https://github.com/akhare101/maven-project'
}
}
stage ('parallel stage')
{
parallel
{
stage('test')
steps{
withMaven(maven : 'localmaven')
{
sh 'mvn test'
}
}
stage('package stage')
steps{
withmaven(maven : 'localmaven')
{
sh 'mvn package'
}
}
}
}
}
}
